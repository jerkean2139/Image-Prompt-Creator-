{
  "name": "promptfusion",
  "version": "1.0.0",
  "description": "Multi-model image generation platform with AI prompt grading and vault system",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:worker\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "vite",
    "dev:worker": "nodemon server/worker.js",
    "build": "prisma migrate deploy && vite build",
    "start": "node server/index.js",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "preview": "vite preview",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@prisma/client": "^6.2.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.28.2",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.2",
    "multer": "^2.0.2",
    "openai": "^4.77.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.3",
    "redis": "^5.10.0",
    "sharp": "^0.33.5",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "prisma": "^6.2.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.7"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "image-generation",
    "prompt-engineering",
    "dalle",
    "flux",
    "ideogram",
    "gemini"
  ],
